#include <main.h>

// Function 1: Add two integers
// operations.c

int add(int a, int b) {
    return (a + b);
}

// Function 2: subtract two integers

int sub(int a, int b) {
    return (a - b);
}

// Function 3: Multiply two integers

int mul(int a, int b) {
    return (a * b);
}

// Function 4: Divide two integers

int div(int a, int b) {
    return (a / b);
}
